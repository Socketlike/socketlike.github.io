{
  "name": "socketlike.github.io",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p types:check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "types:check": "vue-tsc --build --force",
    "eslint:check": "eslint . --ext .vue,.js,.jsx,.cjs.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "eslint:fix": "run-p \"eslint:check --fix\" --",
    "prettier:check": "prettier -c src/",
    "prettier:fix": "prettier -w src/",
    "check": "run-p types:check eslint:check prettier:check"
  },
  "dependencies": {
    "@pictogrammers/memory-svg": "^1.5.0",
    "floating-vue": "^5.2.2",
    "markdown-it": "^14.1.0",
    "pinia": "^2.2.2",
    "typescript": "^5.5.4",
    "vite-svg-loader": "^5.1.0",
    "vue": "^3.4.38",
    "vue-router": "^4.4.3"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.4",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.16.2",
    "@vitejs/plugin-vue": "^5.1.3",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.27.0",
    "less": "^4.2.0",
    "npm-run-all2": "^6.2.2",
    "prettier": "^3.3.3",
    "vite": "^5.4.2",
    "vite-plugin-markdown": "^2.2.0",
    "vue-tsc": "^2.1.2"
  }
}
