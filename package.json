{
  "name": "socketlike.github.io",
  "version": "0.0.1",
  "scripts": {
    "build:dev": "node scripts/build.mjs",
    "build:prod": "node scripts/build.mjs --production",
    "check": "tsc --noEmit",
    "eslint:check": "eslint ./src",
    "prettier:check": "prettier ./src --check",
    "lint": "pnpm run prettier:check && pnpm run eslint:check && pnpm run check",
    "eslint:fix": "eslint ./src --fix",
    "prettier:fix": "prettier ./src --write",
    "lint:fix": "pnpm run prettier:fix && pnpm run eslint:fix"
  },
  "dependencies": {
    "@mdi/js": "^7.2.96",
    "@mdi/react": "^1.6.1",
    "@types/node": "^20.1.7",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "esbuild": "^0.17.19",
    "esbuild-plugin-less": "^1.2.0",
    "eslint": "^8.40.0",
    "eslint-config-dmitmel": "github:dmitmel/eslint-config-dmitmel",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.4",
    "react-dom": "^18.2.0",
    "typescript": "^5.0.4"
  }
}
